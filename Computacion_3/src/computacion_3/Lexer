import static jflexnetbeans.Token.*;
%%
%class Lexer
%type Token
L = [a-zA-Z_]
N = [0-9]
%{
public string lexeme;
public int contador;
%}
%%
/* Reglas lexicas*/

<YYINITIAL>{

L(LL)*	{contador++; lexeme=yytext(); return ID;}
\n|\r 	{}
.	{System.out.println("Token no valido <"+yytext()+ "> linea: " + (yyline+1) + " columna: " +(yycolumn+1));}
}/* Fin yyinitial*/